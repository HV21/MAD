        <activity android:name="com.example.momstime.AddNewScheduleActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCnNDek2tX6mIVGpuNSWgxK3Ts0rfKmaXs" />

        <activity android:name="com.example.momstime.LocationActivity" />
        <activity android:name="com.example.momstime.AddNewNoteActivity" />
